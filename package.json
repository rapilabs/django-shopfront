{
  "name": "django-shopfront",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rm -rf frontend/dist/*",
    "build": "NODE_ENV=production mkdir -p frontend/dist && cp -R frontend/public/* frontend/dist && webpack",
    "heroku-postbuild": "yarn run build",
    "test": "NODE_ENV=testing NODE_PATH=frontend/src mocha --require babel-register --require ignore-styles frontend/test/.setup.js frontend/test/**/*-test.js",
    "lint": "eslint frontend/src"
  },
  "dependencies": {
    "js-cookie": "2.1.4",
    "prop-types": "15.5.8",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-hot-loader": "next",
    "react-redux": "5.0.4",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.4.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "3.5.0",
    "css-loader": "0.28.1",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "fetch-mock": "5.10.0",
    "ignore-styles": "5.0.1",
    "jsdom": "9.12.0",
    "mocha": "3.3.0",
    "node-sass": "4.5.2",
    "react-addons-test-utils": "15.5.1",
    "sass-loader": "6.0.3",
    "sinon": "2.2.0",
    "style-loader": "0.17.0",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.5"
  }
}
